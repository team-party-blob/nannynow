{
  "name": "nannynow",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "preinstall": "yarn clean && yarn run install:client && yarn run install:server",
    "install:client": "cd client && yarn",
    "install:server": "cd server && yarn",
    "test": "yarn test:client && yarn test:server",
    "test:client": "cd client && yarn test",
    "test:server": "cd server && yarn test",
    "build": "yarn build:client && yarn build:server",
    "build:client": "cd client && yarn build",
    "build:server": "cd server && yarn build",
    "prestart": "yarn build",
    "start": "cd server && yarn start",
    "rmpackagelock": "rm -rf package-lock.json",
    "rmnodemodules": "rm -rf node_modules",
    "rmyarnlock": "rm -rf yarn.lock",
    "yarnclean": "yarn rmpackagelock && yarn rmnodemodules && yarn rmyarnlock",
    "cleanserver": "cd server && yarn yarnclean",
    "cleanclient": "cd client && yarn yarnclean",
    "clean": "yarn clean && yarn cleanserver && yarn cleanclient",
    "switchtonpm": "cp package-npm.json package.json"
  },
